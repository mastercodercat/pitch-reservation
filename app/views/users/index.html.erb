<% title "Users" %>

<table>
  <tr>
    <th>NetID</th>
    <th>Name</th>
  </tr>
  <% for user in @users %>
    <tr>
      <td><%= user.login %></td>
      <td><%= link_to user.name, user %></td>
      <td><%= link_to "Edit", edit_user_path(user) %></td>
    </tr>
  <% end %>
</table>

<p><%= link_to "New User", new_user_path %></p>
