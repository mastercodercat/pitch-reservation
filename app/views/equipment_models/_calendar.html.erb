<div class="span6">
  <div id="reservation-calendar">
    <%= content_tag "div", id: "res-data", data:{url:@reservation_data, day:(@date.to_date), max:@max} do %>
    <% end %>
  <ul>
    <% for d in @date...(@date+7.day) %>
        <li class = "calendar_cell">
          <div class = "head">
            <h4></h4>
          </div>
          <div class = "num">
            <h2></h2>
          </div>
        </li>
    <% end %>
  </ul>
  <div class="forward1">forward1</div>
</div>
</div>
