<%= simple_form_for @equipment_object, :html => { :class => "form-horizontal" } do |f| %>
  <%= f.error_notification %>
  <%= f.association :equipment_model, placeholder: 'Please select an equipment model...' %>
  <%=f.input :name%>
  <%=f.input :serial%>
  <%= f.button :submit, :cancel => equipment_objects_path %>
<% end %>
