<%= simple_form_for @equipment_object do |f| %>
  <%= f.error_notification %>
  <%# if @equipment_model %>


    <%#= f.hidden_field :equipment_model_id, :value => @equipment_model.id %>
  <%# else %>
      <%#= f.label :equipment_model_id %>
      <%#= f.select :equipment_model_id, EquipmentModel.not_deleted.select_options, :prompt => true %>
  
  <%# end %>

  <%=f.input :name%>
  <%=f.input :serial%>
  <%=f.input :deleted_at, :as => :deleted, label: "Disabled"%>
  <%=f.button :submit%>
<% end %>
