<h1>Listing black_outs</h1>

<table>
  <tr>
    <th>Equipment model</th>
    <th>Start date</th>
    <th>End date</th>
    <th>Notice</th>
    <th>Created by</th>
    <th>Type</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @black_outs.each do |black_out| %>
  <tr>
    <td><%= black_out.equipment_model %></td>
    <td><%= black_out.start_date %></td>
    <td><%= black_out.end_date %></td>
    <td><%= black_out.notice %></td>
    <td><%= black_out.created_by %></td>
    <td><%= black_out.type %></td>
    <td><%= link_to 'Show', black_out %></td>
    <td><%= link_to 'Edit', edit_black_out_path(black_out) %></td>
    <td><%= link_to 'Destroy', black_out, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

