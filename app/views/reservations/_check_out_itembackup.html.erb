    <% form_for check_out_item do |f| %>
      <br>
      <ul>
        <%= check_out_item.equipment_model.name %>
        <li>
          <%= select_tag 'reservation[equipment_object_id]', check_out_item.equipment_model.available_object_select_options, :prompt => true %>
      <%#= collection_select(:check_out_item, :equipment_object_id, item.equipment_model.equipment_objects, :id, :name, {:prompt => true}) %>
        </li>
      </ul>
      <p>
        This equipment is <strong>due back on <%= check_out_item.due_date.to_date.to_s(:long) %>.</strong><br />
        An overdue return will incur a combined late fee of <%= number_to_currency(check_out_item.late_fee) %> / day.
      </p>
      <p>
        <strong>Checkout procedures:</strong>
        <ul>
          <%= check_out_item.equipment_model.name %>
          <ul>
            <% if check_out_item.equipment_model.checkout_procedures.nil? %>
              no checkout procedures
            <% else %>
              <% check_out_item.equipment_model.checkout_procedures.each do |checkout_procedure| %>
                <li style="list-style: none">
                  <%= check_box_tag 'reservation[checkout_procedures][]' %> <%= checkout_procedure %>
                </li>
              <% end %>
            <% end %>
          </ul>
        </ul>
      </p>
      <%= submit_tag "Check out equipment" %>

    <% end %>

